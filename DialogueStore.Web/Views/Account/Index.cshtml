@model IEnumerable<DialogueStore.Web.Models.User>
@{
    ViewBag.Title = "Users Accounts";
}

<div class="container">

    <div class="row">
        <h2 class="page-header">Settings</h2>
    </div>

    <div class="row">

        <div class="col-md-2">
            <ul class="nav nav-pills nav-stacked">
                <li><a href="@Url.Action("Index", "Settings")">General Settings</a></li>
                <li class="active"><a href="@Url.Action("Index", "Account")">User Accounts</a></li>
                <li><a href="@Url.Action("Create", "Account")">Create User Account</a></li>
            </ul>
        </div>

        <div class="col-md-10">

            <div class="panel panel-default">

                <div class="panel-heading">
                    <h3 class="panel-title">User Accounts</h3>
                </div>

                <div class="panel-body">
                    @Html.Partial("_List", Model)
                    
                    <p>
                        <a href="@Url.Action("Create","Account")" class="btn btn-success btn-sm">Create New</a>
                    </p>
                </div>

            </div>

        </div>

    </div>

</div>
